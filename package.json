{
  "name": "replytics-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "typecheck": "tsc --noEmit --incremental",
    "build:production": "npm run typecheck && npm run lint && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:standalone": "BUILD_STANDALONE=true npm run build:production",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:build": "npm run clean && npm install && npm run build:production",
    "validate": "npm run typecheck && npm run lint",
    "pre-build": "npm run clean && npm run validate",
    "generate-filetree": "node dev-tools/scripts/generate-filetree.js",
    "test": "jest --config=dev-tools/configs/jest.config.js",
    "test:ci": "jest --config=dev-tools/configs/jest.config.js --ci --coverage --watchAll=false",
    "test:integration": "jest --config=dev-tools/configs/jest.config.js --testPathPattern=integration",
    "test:smoke": "playwright test --config=playwright.config.ts",
    "setup-hooks": "pre-commit install"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@supabase/supabase-js": "^2.50.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-query-devtools": "^5.80.7",
    "@types/react-beautiful-dnd": "^13.1.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "framer-motion": "^12.23.1",
    "lucide-react": "^0.363.0",
    "next": "14.2.13",
    "next-auth": "^4.24.7",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.0.3",
    "zod": "^4.0.10",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "playwright": "^1.54.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
