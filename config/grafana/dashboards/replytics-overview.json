{
  "dashboard": {
    "id": null,
    "uid": "replytics-overview",
    "title": "Replytics System Overview",
    "description": "Comprehensive monitoring dashboard for Replytics voice-bot integration platform",
    "tags": ["replytics", "production", "voice-bot", "overview"],
    "timezone": "browser",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "up{job=~\"replytics-.*\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate (req/sec)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "xAxes": [
          {
            "type": "time",
            "show": true
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0,
            "show": true
          },
          {
            "show": true
          }
        ],
        "legend": {
          "avg": false,
          "current": false,
          "max": false,
          "min": false,
          "show": true,
          "total": false,
          "values": false
        }
      },
      {
        "id": 3,
        "title": "Response Time Percentiles",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "90th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0,
            "show": true
          }
        ]
      },
      {
        "id": 4,
        "title": "Error Rate (%)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "5xx Error Rate",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{status=~\"4..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "4xx Error Rate",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100,
            "show": true
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {
                "queryType": "",
                "refId": "A"
              },
              "reducer": {
                "type": "last",
                "params": []
              },
              "evaluator": {
                "params": [5],
                "type": "gt"
              }
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 5,
        "title": "Voice Call Metrics",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(voice_calls_total[5m])",
            "legendFormat": "Calls/sec",
            "refId": "A"
          },
          {
            "expr": "rate(voice_calls_failed_total[5m]) / rate(voice_calls_total[5m]) * 100",
            "legendFormat": "Failure Rate %",
            "refId": "B"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 6,
        "title": "Business ID Cache Performance",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 16
        },
        "targets": [
          {
            "expr": "cache_hit_ratio{cache_type=\"business_id\"}",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            },
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 7,
        "title": "Active WebSocket Connections",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(websocket_connections_active)",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 8,
        "title": "Database Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        },
        "targets": [
          {
            "expr": "rate(database_query_duration_seconds_sum[5m]) / rate(database_query_duration_seconds_count[5m])",
            "legendFormat": "Avg Query Time - {{query_type}}",
            "refId": "A"
          },
          {
            "expr": "database_connections_active",
            "legendFormat": "Active Connections",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds / Count",
            "min": 0,
            "show": true
          }
        ]
      },
      {
        "id": 9,
        "title": "Security Events",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        },
        "targets": [
          {
            "expr": "rate(security_violations_total[5m])",
            "legendFormat": "{{type}} violations/sec",
            "refId": "A"
          },
          {
            "expr": "rate(authentication_failures_total[5m])",
            "legendFormat": "Auth failures/sec",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0,
            "show": true
          }
        ]
      },
      {
        "id": 10,
        "title": "Analytics Query Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 30
        },
        "targets": [
          {
            "expr": "rate(analytics_queries_total[5m])",
            "legendFormat": "{{query_type}} queries/sec",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(analytics_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile duration",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Queries/sec | Seconds",
            "min": 0,
            "show": true
          }
        ]
      },
      {
        "id": 11,
        "title": "System Resource Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 30
        },
        "targets": [
          {
            "expr": "100 - (avg by(instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage % - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory Usage % - {{instance}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100,
            "show": true
          }
        ]
      },
      {
        "id": 12,
        "title": "Customer Interaction Metrics",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 38
        },
        "targets": [
          {
            "expr": "rate(customer_interactions_total[5m])",
            "legendFormat": "{{interaction_type}} - {{outcome}}",
            "refId": "A"
          },
          {
            "expr": "rate(booking_conversions_total[5m])",
            "legendFormat": "Booking Conversions",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Interactions/sec",
            "min": 0,
            "show": true
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "allValue": null,
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "${DS_PROMETHEUS}",
          "definition": "label_values(http_requests_total, business_id)",
          "hide": 0,
          "includeAll": true,
          "label": "Business ID",
          "multi": true,
          "name": "business_id",
          "options": [],
          "query": "label_values(http_requests_total, business_id)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        },
        {
          "allValue": null,
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "${DS_PROMETHEUS}",
          "definition": "label_values(up, job)",
          "hide": 0,
          "includeAll": true,
          "label": "Service",
          "multi": true,
          "name": "service",
          "options": [],
          "query": "label_values(up, job)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "links": [],
    "style": "dark",
    "hideControls": false
  }
}