# Application Settings
DEBUG=false
PORT=8000

# Security - IMPORTANT: Generate a secure random key for production
SECRET_KEY=REPLACE_WITH_STRONG_RANDOM_SECRET_KEY_MINIMUM_32_CHARACTERS

# Database - Get these from your Supabase project settings
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Voice Bot Integration
VOICE_BOT_URL=https://replytics-dhhf.onrender.com
VOICE_BOT_TIMEOUT=30
VOICE_BOT_MAX_RETRIES=3
VOICE_BOT_CACHE_TTL=300
VOICE_BOT_WEBHOOK_SECRET=your-webhook-secret-for-signature-verification

# Voice Bot JWT Configuration
VOICE_BOT_JWT_SECRET=your-voice-bot-jwt-secret-generate-random-64-chars
VOICE_BOT_JWT_ALGORITHM=HS256
VOICE_BOT_JWT_EXPIRE_MINUTES=30
VOICE_BOT_INTERNAL_TOKEN=your-voice-bot-internal-token-generate-random-64-chars

# Redis (Optional - for caching Voice Bot responses)
REDIS_URL=redis://localhost:6379

# External Services
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Google OAuth - Get these from Google Cloud Console
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Stripe Payment Processing - Get these from your Stripe Dashboard
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
STRIPE_STARTER_PRICE_ID=price_your_starter_plan_price_id
STRIPE_PROFESSIONAL_PRICE_ID=price_your_professional_plan_price_id
STRIPE_ENTERPRISE_PRICE_ID=price_your_enterprise_plan_price_id

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.com

