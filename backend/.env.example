# Application Settings
DEBUG=false
PORT=8000

# Security - IMPORTANT: Generate a secure random key for production
SECRET_KEY=REPLACE_WITH_STRONG_RANDOM_SECRET_KEY_MINIMUM_32_CHARACTERS

# Database - Get these from your Supabase project settings
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# External Services
VOICE_BOT_URL=https://your-voice-bot-url.com

# Google OAuth - Get these from Google Cloud Console
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.com
